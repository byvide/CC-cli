{
	"deno.enable": true,

	"editor.defaultFormatter": "denoland.vscode-deno",
	"editor.formatOnSave": true,
	"editor.codeActionsOnSave": {
		"source.fixAll": "explicit",
		"source.organizeImports": "explicit"
	},

	"workbench.colorCustomizations": {
		"activityBar.activeBackground": "#f25a01",
		"activityBar.background": "#f25a01",
		"activityBar.foreground": "#e7e7e7",
		"activityBar.inactiveForeground": "#e7e7e799",
		"activityBarBadge.background": "#005f23",
		"activityBarBadge.foreground": "#e7e7e7",
		"commandCenter.border": "#e7e7e799",
		"sash.hoverBorder": "#f25a01",
		"statusBar.background": "#bf4701",
		"statusBar.foreground": "#e7e7e7",
		"statusBarItem.hoverBackground": "#f25a01",
		"statusBarItem.remoteBackground": "#bf4701",
		"statusBarItem.remoteForeground": "#e7e7e7",
		"titleBar.activeBackground": "#bf4701",
		"titleBar.activeForeground": "#e7e7e7",
		"titleBar.inactiveBackground": "#bf470199",
		"titleBar.inactiveForeground": "#e7e7e799"
	},
	"peacock.color": "#bf4701"
}
